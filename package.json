{
  "name": "vercel-file-fusion",
  "version": "0.1.0",
  "description": "Quick batch processing of locale files.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/vercel-file-fusion.git"
  },
  "scripts": {
    "commit": "git-cz",
    "ci:coverage": "pnpm test",
    "postinstall": "node --experimental-modules ./scripts/postinstall.mjs",
    "preinstall": "node --experimental-modules ./scripts/preinstall.mjs",
    "clear": "rimraf --glob ./libs",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "lint": "pnpm lint:ts",
    "lint:ts": "eslint -c eslint.config.mjs --fix .",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:unitest": "jest -c ./jest.config.ts --coverage --reporters=default --reporters=jest-junit --passWithNoTests",
    "test:unitest:watch": "jest --watch",
    "test": "pnpm run test:unitest",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:auto": "playwright test --reporter=list",
    "ok": "pnpm format && pnpm lint && pnpm build && pnpm test",
    "dev:e2e": "next dev --hostname 127.0.0.1 --port 3000",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/classnames": "^2.3.4",
    "@types/pdf-parse": "^1.1.4",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@unrar-browser/core": "1.0.3-alpha.1763050400.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@zip.js/zip.js": "^2.7.57",
    "ahooks": "^3.8.4",
    "classnames": "^2.5.1",
    "decimal.js": "^10.5.0",
    "feather-icons-react": "^0.7.1",
    "iconv-lite": "^0.6.3",
    "music-metadata": "^10.9.0",
    "next": "15.5.7",
    "opencc-js": "^1.0.5",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "pnpm": "^10.4.0",
    "react": "^19.1.2",
    "react-dom": "^19.1.2",
    "react-dropzone": "^14.3.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.56.1",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "chalk": "^5.3.0",
    "commitizen": "^4.2.4",
    "commitlint-plugin-i18n": "^0.0.1-alpha.3",
    "cz-customizable": "^7.2.1",
    "cz-customizable-i18n-config": "^0.0.1-alpha.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-runner-tsd": "^6.0.0",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "pretty-error": "^4.0.0",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4",
    "spinnies": "^0.5.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsd-lite": "^0.9.0",
    "typescript": "^5",
    "typescript-transform-paths": "^3.4.7"
